{
  "name": "cordium.js",
  "version": "0.0.1",
  "description": "A multi-threaded vertically scalable library for interacting with the Discord API.",
  "main": "build/index.js",
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@types/node": "^20.14.2",
    "@types/ws": "^8.5.10",
    "dotenv": "^16.4.5",
    "eslint": "^9.0.0",
    "globals": "^15.6.0",
    "sucrase": "^3.35.0",
    "tsup": "^8.1.0",
    "typedoc": "^0.26.3",
    "typedoc-plugin-markdown": "^4.1.0",
    "typescript": "^5.5.2",
    "typescript-eslint": "^7.13.1"
  },
  "dependencies": {
    "ws": "^8.17.0"
  },
  "scripts": {
    "dev": "node -r sucrase/register",
    "build": "tsup",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "docs:generate": "rm -rf ./docs/pages/ && typedoc && chmod +x ./scripts/organizeDocs.sh && ./scripts/organizeDocs.sh",
    "docs:build": "cd docs && npm run build",
    "docs:serve": "cd docs && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devdimer/cordium.js.git"
  },
  "keywords": [
    "discord",
    "discordjs",
    "discord.js",
    "discordbot",
    "discordapp",
    "wrapper",
    "multi-threaded",
    "clusterized",
    "api"
  ],
  "author": "vonderheimsk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devdimer/cordium.js/issues"
  },
  "homepage": "https://cordiumjs.vercel.app"
}
